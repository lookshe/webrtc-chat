{
  /* The ID of the node instance */
  "nodeId" : 1,
  
  /* The port to listen on */
  "port" : 4500,  
  
  /* Allow anonymous connections */
  "anonymous" : false,  
    
  /* Redis configuration (required if anonymous == false) */
  "redis" : {
    "host" : "127.0.0.1",
    "port" : 6379,
    "password" : ""
  },
  
  /* SSL configuration (required if using HTTPS) */
  "ssl" : {
    "enabled" : false,
    "key" : "/path/to/.key",
    "cert" : "/path/to/.crt"
  }
}